{
	"default-remote":"anutron",
	"remotes": {
		"anutron": {
			"fetch": "git://github.com/anutron/%(name)s.git"
		},
		"mootools": {
			"fetch": "git://github.com/mootools/%(name)s.git"
		},
		"behavior-ui": {
			"fetch": "git://github.com/behavior-ui/%(name)s.git"
		},
		"origin": {
			"fetch": "git@github.com:anutron/%(name)s.git",
			"push": "git@github.com:anutron/%(name)s.git"
		}
	},
	"projects": {
		"core": {
			"dir":"core",
			"remote-project-name": "mootools-core",
			"track-indirect": "core.hash",
			"remotes": ["mootools"]
		},
		"more": {
			"dir":"more",
			"remote-project-name": "mootools-more",
			"track-indirect": "more.hash",
			"remotes": ["mootools"]
		},
		"clientcide": {
			"dir": "clientcide",
			"track-branch": "master",
			"from-remote": "anutron",
			"remotes": ["anutron", "origin"]
		},
		"behavior": {
			"track-branch": "master",
			"from-remote": "anutron",
			"remotes": ["anutron", "origin"]
		},
		"more-behaviors": {
			"track-branch": "master",
			"from-remote": "anutron",
			"remotes": ["anutron", "origin"]
		},
		"mootools-bootstrap": {
			"dir": "bootstrap",
			"track-branch": "master",
			"from-remote": "anutron",
			"remotes": ["anutron", "origin"]
		},
		"behavior-ui": {
			"dir": "behavior-ui",
			"track-branch": "master",
			"from-remote": "behavior-ui",
			"remotes": ["behavior-ui"]
		}
	}
}
