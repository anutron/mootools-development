{
	"default-remote":"mootools",
	"remotes": {
		"mootools": {
			"fetch": "git://github.com/mootools/%(name)s.git"
		}
	},
	"projects": {
		"mootools-core": {
			"dir":"core",
			"track-branch": "master"
		},
		"mootools-more": {
			"dir":"more",
			"track-branch": "master"
		},
		"mootools-demos": {
			"dir":"demos",
			"track-branch": "master"
		},
		"mootools-core-specs": {
			"dir":"core-specs",
			"track-branch": "master"
		}
	}
}
